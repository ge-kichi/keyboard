{
  "name": "keyboard",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@l1ck0h/every-layout.css": "git+ssh://git@github.com:l1ck0h/every-layout.css.git",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@tonejs/midi": "^2.0.27",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.21",
    "@types/ramda": "^0.28.1",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "ramda": "^0.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "spectre.css": "^0.5.9",
    "tone": "^14.7.77",
    "typescript": "^4.4.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "yarn build && gh-pages -d build",
    "lint:eslint": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "lint:prettier": "prettier --check --write './src/**/*.{css,js,jsx,ts,tsx}'",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/node-logger": "^6.4.19",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.4.1"
  },
  "homepage": "https://l1ck0h.github.io/keyboard/",
  "resolutions": {
    "postcss-custom-properties": "^12.1.4"
  }
}
